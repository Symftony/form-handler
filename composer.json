{
    "name": "symftony/form-handler",
    "type": "library",
    "description": "A Symfony form handler",
    "keywords": [
        "form",
        "handler",
        "symfony",
        "bundle"
    ],
    "homepage": "https://github.com/symftony/form-handler",
    "require": {
        "php": ">=5.5.9",
        "symfony/form": ">=2.8 <4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "symfony/dependency-injection": ">=2.8",
        "symfony/http-kernel": ">=2.8",
        "symfony/validator": ">=2.8"
    },
    "suggest": {
        "symfony/validator": ">=2.8",
        "symfony/serializer": ">=2.8"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony",
            "email": "symftony@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Symftony\\FormHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symftony\\FormHandler\\Tests\\": "tests/"
        }
    }
}
